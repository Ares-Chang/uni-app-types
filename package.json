{
  "name": "@uni-helper/uni-app-types",
  "version": "0.4.0",
  "description": "uni-app 组件类型",
  "keywords": [
    "uni-app",
    "uniapp",
    "type"
  ],
  "homepage": "https://github.com/uni-helper/uni-app-types#readme",
  "bugs": {
    "url": "https://github.com/uni-helper/uni-app-types/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uni-helper/uni-app-types.git"
  },
  "license": "MIT",
  "author": {
    "name": "ModyQyW",
    "email": "wurui-dev@foxmail.com",
    "url": "https://modyqyw.github.io"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "check:deps": "ncu --dep dev,optional,prod,packageManager",
    "check:types": "tsc --noEmit",
    "dev": "rollup -c --watch",
    "lint": "run-p lint:eslint lint:markdownlint lint:publint check:types test:coverage",
    "lint:eslint": "eslint ./src --fix --cache",
    "lint:markdownlint": "markdownlint . --fix --ignore-path=.gitignore",
    "lint:publint": "publint",
    "prepare": "is-ci || husky install",
    "release": "pnpm install && pnpm run lint && bumpp",
    "test": "vitest typecheck --run",
    "test:coverage": "vitest typecheck --run --coverage"
  },
  "dependencies": {
    "@dcloudio/types": "^3.2.11",
    "vue3": "npm:vue@^3.2.47"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.3",
    "@commitlint/config-conventional": "^17.4.3",
    "@commitlint/prompt": "^17.4.3",
    "@modyqyw/fabric": "^7.0.3",
    "@modyqyw/utils": "^5.0.0",
    "@tsconfig/node18": "^1.0.1",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@typescript-eslint/parser": "^5.52.0",
    "@vitest/coverage-c8": "^0.28.5",
    "bumpp": "^8.2.1",
    "c8": "^7.12.0",
    "commitizen": "^4.3.0",
    "eslint": "^8.34.0",
    "husky": "^8.0.3",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.1.2",
    "markdownlint-cli": "^0.33.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "publint": "^0.1.10",
    "rollup": "^3.18.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^5.2.0",
    "typescript": "^4.9.5",
    "vitest": "^0.28.5"
  },
  "peerDependencies": {
    "typescript": "^4.8.0"
  },
  "packageManager": "pnpm@7.27.0",
  "engines": {
    "node": ">=14.18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
